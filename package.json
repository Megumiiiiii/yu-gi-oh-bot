{
  "name": "discord-bot",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc && node .",
    "build": "tsc",
    "dev": "nodemon index.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^15.6.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "discord.js": "^13.0.1",
    "dotenv": "^10.0.0",
    "glob": "^7.1.7",
    "nodemon": "^2.0.7",
    "paralogger": "^0.1.0"
  }
}
